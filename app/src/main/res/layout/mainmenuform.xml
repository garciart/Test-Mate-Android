<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/IDD_MAINMENUFORM_LINEARLAYOUT"
	android:background="#FFFFFF"
	android:layout_height="fill_parent"
	android:layout_width="fill_parent"
	android:orientation="vertical">
	<ImageView
		android:id="@+id/IDC_MAIN_PICTUREBOX"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:src="@drawable/tmbanner360" />
	<Button
		android:id="@+id/IDC_MAIN_BUTTON_STARTANEWTEST"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:text="Start a New Test..."
		android:textColor="#FFFFFF"
		android:textStyle="bold"
		android:background="@drawable/custom_button"/>
	<Button
		android:id="@+id/IDC_MAIN_BUTTON_CONTINUEATEST"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:text="Continue a Test..."
		android:textColor="#FFFFFF"
		android:textStyle="bold"
		android:background="@drawable/custom_button"/>
	<Button
		android:id="@+id/IDC_MAIN_BUTTON_ABOUTTESTMATE"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:text="About Test Mate..."
		android:textColor="#FFFFFF"
		android:textStyle="bold"
		android:background="@drawable/custom_button"/>
	<Button
		android:id="@+id/IDC_MAIN_BUTTON_EXITTESTMATE"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:text="Exit Test Mate..."
		android:textColor="#FFFFFF"
		android:textStyle="bold"
		android:background="@drawable/custom_button"/>
</LinearLayout>
